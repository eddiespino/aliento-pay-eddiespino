{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/validate/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/calculate/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/curation-stats/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/filters/?$",
			"dest": "_render"
		},
		{
			"src": "^/calculate/?$",
			"dest": "_render"
		},
		{
			"src": "^/dashboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/payments/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}